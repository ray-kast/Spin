res {
  h1-size = 20;

  style Header {
    font-weight = "bold";
  }

  style H1: Header {
    tag = "h1";
    font-size = h1-size;
  }

  style Em { tag = "em"; }
}

Html {
  title = "hi";

  Body {
    Div {
      Label: H1 { "Hiya!" }

      "Hello World!"

      "Here's some "

      Label: Em { "interpolated" }

      " text!"
    }
  }
}
